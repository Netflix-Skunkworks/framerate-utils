{
  "name": "framerate-utils",
  "version": "4.2.1",
  "description": "Utilities to handle frame rate conversion and display",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tbranyen/framerate-utils"
  },
  "author": "Andy Swan (aswan@netflix.com)",
  "contributors": [
    {
      "name": "Tim Branyen (@tbranyen)"
    }
  ],
  "license": "MIT",
  "module": "dist/framerate-utils.esm.js",
  "size-limit": [
    {
      "path": "dist/framerate-utils.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/framerate-utils.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^6.0.4",
    "husky": "^7.0.4",
    "prettier": "^2.7.1",
    "size-limit": "^6.0.4",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^4.4.4"
  }
}
